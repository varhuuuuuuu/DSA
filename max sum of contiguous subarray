#include <stdio.h>

int main()
{
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter array elements:\n");
    for(int i = 0; i < n; i++){
        scanf("%d", &arr[i]);
    }

    int maxSum = arr[0];

    for(int i = 0; i < n; i++){
        int currentSum = 0;
        for(int j = i; j < n; j++){
            currentSum = currentSum + arr[j];
            if(currentSum > maxSum){
                maxSum = currentSum;
            }
        }
    }
    printf("Maximum Subarray Sum = %d", maxSum);
    return 0;
}
